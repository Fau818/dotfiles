{
  "title": "Basic Configurations",
  "rules": [
    {
      "description": "Better <TAB> <L_CTRL> <CAPS_LOCK>",
      "manipulators": [
        {
          "description": "<TAB> := <L_OPTION>",
          "type": "basic",
          "conditions": [{ "type": "event_changed_if", "value": false }],
          "from": { "key_code": "tab", "modifiers": { "mandatory": [], "optional": ["caps_lock", "right_control"] } },
          "to": [{ "key_code": "left_option", "lazy": true }],
          "to_if_alone": [{ "key_code": "tab" }]
        },

        {
          "description": "Alone<L_CTRL> := <CAPS_LOCK>",
          "type": "basic",
          "conditions": [{ "type": "event_changed_if", "value": false }],
          "from": { "key_code": "left_control", "modifiers": { "mandatory": [], "optional": ["caps_lock", "left_option"] } },
          "to": [{ "key_code": "left_control", "lazy": true }],
          "to_if_alone": [{ "key_code": "caps_lock" }]
        },

        {
          "description": "<CAPS_LOCK> := <ESC> and <R_CTRL>",
          "type": "basic",
          "conditions": [{ "type": "event_changed_if", "value": false }],
          "from": { "key_code": "caps_lock", "modifiers": { "mandatory": [], "optional": ["any"] } },
          "to": [{ "key_code": "right_control", "lazy": true }],
          "to_if_alone": [{ "key_code": "escape" }]
        },

        {
          "description": "<R_CTRL><SPACE> := <CAPS_LOCK>",
          "type": "basic",
          "from": { "key_code": "spacebar", "modifiers": { "mandatory": ["right_control"], "optional": ["caps_lock", "left_option"] } },
          "to": [{ "key_code": "caps_lock" }]
        },


        {
          "description": "<R_CTRL>h := <LEFT>",
          "type": "basic",
          "from": { "key_code": "h", "modifiers": { "mandatory": ["right_control"], "optional": ["any"] } },
          "to": [{ "key_code": "left_arrow" }]
        },

        {
          "description": "<R_CTRL>l := <RIGHT>",
          "type": "basic",
          "from": { "key_code": "l", "modifiers": { "mandatory": ["right_control"], "optional": ["any"] } },
          "to": [{ "key_code": "right_arrow" }]
        },

        {
          "description": "<R_CTRL>j := <DOWN>",
          "type": "basic",
          "from": { "key_code": "j", "modifiers": { "mandatory": ["right_control"], "optional": ["any"] } },
          "to": [{ "key_code": "down_arrow" }]
        },

        {
          "description": "<R_CTRL>k := <UP>",
          "type": "basic",
          "from": { "key_code": "k", "modifiers": { "mandatory": ["right_control"], "optional": ["any"] } },
          "to": [{ "key_code": "up_arrow" }]
        },


        {
          "description": "<R_CTRL>1 := <F1>",
          "type": "basic",
          "from": { "key_code": "1", "modifiers": { "mandatory": ["right_control"], "optional": ["any"] } },
          "to": [{ "key_code": "f1", "modifiers": ["fn"] }]
        },

        {
          "description": "<R_CTRL>2 := <F2>",
          "type": "basic",
          "from": { "key_code": "2", "modifiers": { "mandatory": ["right_control"], "optional": ["any"] } },
          "to": [{ "key_code": "f2", "modifiers": ["fn"] }]
        },

        {
          "description": "<R_CTRL>3 := <F3>",
          "type": "basic",
          "from": { "key_code": "3", "modifiers": { "mandatory": ["right_control"], "optional": ["any"] } },
          "to": [{ "key_code": "f3", "modifiers": ["fn"] }]
        },

        {
          "description": "<R_CTRL>4 := <F4>",
          "type": "basic",
          "from": { "key_code": "4", "modifiers": { "mandatory": ["right_control"], "optional": ["any"] } },
          "to": [{ "key_code": "f4", "modifiers": ["fn"] }]
        },

        {
          "description": "<R_CTRL>5 := <F5>",
          "type": "basic",
          "from": { "key_code": "5", "modifiers": { "mandatory": ["right_control"], "optional": ["any"] } },
          "to": [{ "key_code": "f5", "modifiers": ["fn"] }]
        },

        {
          "description": "<R_CTRL>6 := <F6>",
          "type": "basic",
          "from": { "key_code": "6", "modifiers": { "mandatory": ["right_control"], "optional": ["any"] } },
          "to": [{ "key_code": "f6", "modifiers": ["fn"] }]
        },

        {
          "description": "<R_CTRL>7 := <F7>",
          "type": "basic",
          "from": { "key_code": "7", "modifiers": { "mandatory": ["right_control"], "optional": ["any"] } },
          "to": [{ "key_code": "f7", "modifiers": ["fn"] }]
        },

        {
          "description": "<R_CTRL>8 := <F8>",
          "type": "basic",
          "from": { "key_code": "8", "modifiers": { "mandatory": ["right_control"], "optional": ["any"] } },
          "to": [{ "key_code": "f8", "modifiers": ["fn"] }]
        },

        {
          "description": "<R_CTRL>9 := <F9>",
          "type": "basic",
          "from": { "key_code": "9", "modifiers": { "mandatory": ["right_control"], "optional": ["any"] } },
          "to": [{ "key_code": "f9", "modifiers": ["fn"] }]
        },

        {
          "description": "<R_CTRL>0 := <F10>",
          "type": "basic",
          "from": { "key_code": "0", "modifiers": { "mandatory": ["right_control"], "optional": ["any"] } },
          "to": [{ "key_code": "f10", "modifiers": ["fn"] }]
        }
      ]
    },



    {
      "description": "Better <L_CMD> and Fix Touch ID Key",
      "manipulators": [
        {
          "description": "Alone<L_CMD> = <LEFT_HYPER><UP>(Mission Control)",
          "type": "basic",
          "conditions": [{ "type": "event_changed_if", "value": false }],
          "from": { "key_code": "left_command", "modifiers": { "mandatory": [], "optional": ["caps_lock"] } },
          "to": [{ "key_code": "left_command", "lazy": true }],
          "to_if_alone": [{ "key_code": "up_arrow", "modifiers": ["left_control", "left_shift", "left_command", "left_option"] }]
        },

        {
          "description": "Fix Touch ID Key in Locking Screen",
          "type": "basic",
          "conditions": [{ "type": "event_changed_if", "value": false }],
          "from": { "consumer_key_code": "al_terminal_lock_or_screensaver", "modifiers": { "mandatory": [], "optional": ["caps_lock"] } },
          "to": [{ "key_code": "q", "modifiers": ["left_control", "left_command"] }]
        }
      ]
    },



    {
      "description": "Window Keys",
      "manipulators": [
        {
          "description": "~ := ~ and <WINKEY1>",
          "type": "basic",
          "conditions": [{ "type": "event_changed_if", "value": false }],
          "from": { "key_code": "grave_accent_and_tilde", "modifiers": { "mandatory": [], "optional": ["caps_lock", "fn"] } },
          "to": [{ "key_code": "right_shift", "modifiers": ["left_option"], "lazy": true }],
          "to_if_alone": [{ "key_code": "grave_accent_and_tilde" }]
        },

        {
          "description": "<ESC> := <L_CTRL><SPACE> and <WINKEY2>",
          "type": "basic",
          "conditions": [{ "type": "event_changed_if", "value": false }],
          "from": { "key_code": "escape", "modifiers": { "mandatory": [], "optional": ["caps_lock", "fn"] } },
          "to": [{ "key_code": "right_shift", "modifiers": ["left_control", "left_option"], "lazy": true }],
          "to_if_alone": [{ "key_code": "spacebar", "modifiers": ["left_control"] }]
        },

        {
          "description": "<L_SHIFT>~ := <WINKEY3>(<FN><WINKEY1>)",
          "type": "basic",
          "conditions": [{ "type": "event_changed_if", "value": false }],
          "from": { "key_code": "grave_accent_and_tilde", "modifiers": { "mandatory": ["left_shift"], "optional": ["caps_lock"] } },
          "to": [{ "key_code": "fn", "modifiers": ["right_shift", "left_option"], "lazy": true }]
        },

        {
          "description": "<ESC>~ := <WINKEY4>(<FN><WINKEY2>)",
          "type": "basic",
          "conditions": [{ "type": "event_changed_if", "value": false }],
          "from": { "key_code": "grave_accent_and_tilde", "modifiers": { "mandatory": ["right_shift", "left_control", "left_option"], "optional": ["caps_lock"] } },
          "to": [{ "key_code": "fn", "modifiers": ["right_shift", "left_control", "left_option"], "lazy": true }]
        },
        {
          "description": "~<ESC> := <WINKEY4>(<FN><WINKEY2>)",
          "type": "basic",
          "conditions": [{ "type": "event_changed_if", "value": false }],
          "from": { "key_code": "escape", "modifiers": { "mandatory": ["right_shift", "left_option"], "optional": ["caps_lock"] } },
          "to": [{ "key_code": "fn", "modifiers": ["right_shift", "left_control", "left_option"], "lazy": true }]
        },
        {
          "description": "<L_SHIFT><ESC> := <WINKEY4>(<FN><WINKEY2>)",
          "type": "basic",
          "conditions": [{ "type": "event_changed_if", "value": false }],
          "from": { "key_code": "escape", "modifiers": { "mandatory": ["left_shift"], "optional": ["caps_lock"] } },
          "to": [{ "key_code": "fn", "modifiers": ["right_shift", "left_control", "left_option"], "lazy": true }]
        }
      ]
    }
  ]
}

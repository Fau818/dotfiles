{
  "title": "Hyper Open",
  "rules": [
    {
      "description": "<R_CMD> := <HYPER> and <R_OPT> := <HHYPER>",
      "manipulators": [
        {
          "description": "<R_CMD> := <HYPER> with sticky",
          "type": "basic",
          "conditions": [{ "type": "event_changed_if", "value": false }],
          "from": { "key_code": "right_command", "modifiers": { "mandatory": [], "optional": ["any"] } },
          "to": [{ "key_code": "right_command", "modifiers": ["left_control", "left_shift", "left_option"], "lazy": true }],
          "to_if_alone": [{ "sticky_modifier": { "right_command": "toggle" } }, { "sticky_modifier": { "left_control": "toggle" } }, { "sticky_modifier": { "left_shift": "toggle" } }, { "sticky_modifier": { "left_option": "toggle" } }]
        },

        {
          "description": "<R_OPT> := <HHYPER> with sticky",
          "type": "basic",
          "conditions": [{ "type": "event_changed_if", "value": false }],
          "from": { "key_code": "right_option", "modifiers": { "mandatory": [], "optional": ["any"] } },
          "to": [{ "key_code": "right_option", "modifiers": ["left_control", "left_shift", "left_command"], "lazy": true }],
          "to_if_alone": [{ "sticky_modifier": { "right_option": "toggle" } }, { "sticky_modifier": { "left_control": "toggle" } }, { "sticky_modifier": { "left_shift": "toggle" } }, { "sticky_modifier": { "left_command": "toggle" } }]
        },


        {
          "description": "Revise <R_CMD><BK> := <L_CMD><BK>",
          "type": "basic",
          "conditions": [{ "type": "event_changed_if", "value": false }],
          "from": { "key_code": "delete_or_backspace", "modifiers": { "mandatory": ["right_command", "left_control", "left_shift", "left_option"], "optional": ["caps_lock"] } },
          "to": [{ "key_code": "delete_or_backspace", "modifiers": ["left_command"] }]
        },

        {
          "description": "Revise <R_CMD>/<R_OPT>, := <NONE>",
          "type": "basic",
          "from": { "key_code": "comma", "modifiers": { "mandatory": ["shift", "control", "option", "command"], "optional": ["any"] } },
          "to": [{ "key_code": "vk_none", "halt": true }]
        },

        {
          "description": "Revise <R_CMD>/<R_OPT>. := <NONE>",
          "type": "basic",
          "from": { "key_code": "period", "modifiers": { "mandatory": ["shift", "control", "option", "command"], "optional": ["any"] } },
          "to": [{ "key_code": "vk_none", "halt": true }]
        }
      ]
    },



    {
      "description": "<HYPER> and <HHYPER> Open",
      "manipulators": [
        {
          "description": "<HYPER><SPACE> := Play Music",
          "type": "basic",
          "from": { "key_code": "spacebar", "modifiers": { "mandatory": ["right_command", "left_control", "left_shift", "left_option"], "optional": ["caps_lock"] } },
          "to": [{ "key_code": "vk_none" }, { "shell_command": "osascript -e 'tell application \"Music\" to playpause'" }]
        },

        {
          "description": "<HYPER>b := open Bob",
          "type": "basic",
          "from": { "key_code": "b", "modifiers": { "mandatory": ["right_command", "left_control", "left_shift", "left_option"], "optional": ["caps_lock"] } },
          "to": [{ "key_code": "vk_none" }, { "shell_command": "open -a Bob && open -a Bob" }]
        },

        {
          "description": "<HYPER>d := open Dash",
          "type": "basic",
          "from": { "key_code": "d", "modifiers": { "mandatory": ["right_command", "left_control", "left_shift", "left_option"], "optional": ["caps_lock"] } },
          "to": [{ "key_code": "vk_none" }, { "shell_command": "open -a Dash" }]
        },

        {
          "description": "<HYPER>f := open Finder",
          "type": "basic",
          "from": { "key_code": "f", "modifiers": { "mandatory": ["right_command", "left_control", "left_shift", "left_option"], "optional": ["caps_lock"] } },
          "to": [{ "key_code": "vk_none" }, { "shell_command": "open -a Finder" }]
        },

        {
          "description": "<HYPER>g := open Google Chrome",
          "type": "basic",
          "from": { "key_code": "g", "modifiers": { "mandatory": ["right_command", "left_control", "left_shift", "left_option"], "optional": ["caps_lock"] } },
          "to": [{ "key_code": "vk_none" }, { "shell_command": "open -a Google\\ Chrome" }]
        },

        {
          "description": "<HYPER>m := open Apple Music",
          "type": "basic",
          "from": { "key_code": "m", "modifiers": { "mandatory": ["right_command", "left_control", "left_shift", "left_option"], "optional": ["caps_lock"] } },
          "to": [{ "key_code": "vk_none" }, { "shell_command": "open -a Music" }]
        },

        {
          "description": "<HYPER>o := open Obsidian",
          "type": "basic",
          "from": { "key_code": "o", "modifiers": { "mandatory": ["right_command", "left_control", "left_shift", "left_option"], "optional": ["caps_lock"] } },
          "to": [{ "key_code": "vk_none" }, { "shell_command": "open -a Obsidian" }]
        },

        {
          "description": "<HYPER>r := open Reminders",
          "type": "basic",
          "from": { "key_code": "r", "modifiers": { "mandatory": ["right_command", "left_control", "left_shift", "left_option"], "optional": ["caps_lock"] } },
          "to": [{ "key_code": "vk_none" }, { "shell_command": "open -a Reminders" }]
        },

        {
          "description": "<HYPER>s := open Browser (Arc)",
          "type": "basic",
          "from": { "key_code": "s", "modifiers": { "mandatory": ["right_command", "left_control", "left_shift", "left_option"], "optional": ["caps_lock"] } },
          "to": [{ "key_code": "vk_none" }, { "shell_command": "open -a Arc" }]
        },

        {
          "description": "<HYPER>t := open Terminal (Kitty)",
          "type": "basic",
          "from": { "key_code": "t", "modifiers": { "mandatory": ["right_command", "left_control", "left_shift", "left_option"], "optional": ["caps_lock"] } },
          "to": [{ "key_code": "vk_none" }, { "shell_command": "open -a kitty" }]
        },

        {
          "description": "<HYPER>v := open VSCode",
          "type": "basic",
          "from": { "key_code": "v", "modifiers": { "mandatory": ["right_command", "left_control", "left_shift", "left_option"], "optional": ["caps_lock"] } },
          "to": [{ "key_code": "vk_none" }, { "shell_command": "open -a Visual\\ Studio\\ Code" }]
        },


        {
          "description": "<HHYPER>d := open Downie4",
          "type": "basic",
          "from": { "key_code": "d", "modifiers": { "mandatory": ["right_option", "left_control", "left_shift", "left_command"], "optional": ["caps_lock"] } },
          "to": [{ "key_code": "vk_none" }, { "shell_command": "open -a Downie\\ 4" }]
        },

        {
          "description": "<HHYPER>m := open Mail",
          "type": "basic",
          "from": { "key_code": "m", "modifiers": { "mandatory": ["right_option", "left_control", "left_shift", "left_command"], "optional": ["caps_lock"] } },
          "to": [{ "key_code": "vk_none" }, { "shell_command": "open -a Mail" }]
        },

        {
          "description": "<HHYPER>s := open Browser (Safari)",
          "type": "basic",
          "from": { "key_code": "s", "modifiers": { "mandatory": ["right_option", "left_control", "left_shift", "left_command"], "optional": ["caps_lock"] } },
          "to": [{ "key_code": "vk_none" }, { "shell_command": "open -a Safari" }]
        },

        {
          "description": "<HHYPER>t := open Terminal (Termius)",
          "type": "basic",
          "from": { "key_code": "t", "modifiers": { "mandatory": ["right_option", "left_control", "left_shift", "left_command"], "optional": ["caps_lock"] } },
          "to": [{ "key_code": "vk_none" }, { "shell_command": "open -a Termius" }]
        }
      ]
    },



    {
      "description": "<FN> := <FN> with sticky and <FN> Open",
      "manipulators": [
        {
          "description": "<FN> := <FN> with sticky",
          "type": "basic",
          "conditions": [{ "type": "event_changed_if", "value": false }],
          "from": { "key_code": "fn", "modifiers": { "mandatory": [], "optional": ["caps_lock", "fn"] } },
          "to": [{ "key_code": "fn", "lazy": true }],
          "to_if_alone": [{ "sticky_modifier": { "fn": "toggle" } }],
          "to_if_held_down": [{ "key_code": "fn" }]
        },

        {
          "description": "<FN>a := open WhatsApp",
          "type": "basic",
          "from": { "key_code": "a", "modifiers": { "mandatory": ["fn"], "optional": ["caps_lock"] } },
          "to": [{ "key_code": "vk_none" }, { "shell_command": "open -a WhatsApp" }]
        },

        {
          "description": "<FN>a := open Messages",
          "type": "basic",
          "from": { "key_code": "s", "modifiers": { "mandatory": ["fn"], "optional": ["caps_lock"] } },
          "to": [{ "key_code": "vk_none" }, { "shell_command": "open -a Messages" }]
        },

        {
          "description": "<FN>d := open Discord",
          "type": "basic",
          "from": { "key_code": "d", "modifiers": { "mandatory": ["fn"], "optional": ["caps_lock"] } },
          "to": [{ "key_code": "vk_none" }, { "shell_command": "open -a Discord" }]
        },

        {
          "description": "<FN>q := open QQ",
          "type": "basic",
          "from": { "key_code": "q", "modifiers": { "mandatory": ["fn"], "optional": ["caps_lock"] } },
          "to": [{ "key_code": "vk_none" }, { "shell_command": "open -a QQ" }]
        },

        {
          "description": "<FN>t := open Telegram",
          "type": "basic",
          "from": { "key_code": "t", "modifiers": { "mandatory": ["fn"], "optional": ["caps_lock"] } },
          "to": [{ "key_code": "vk_none" }, { "shell_command": "open -a Telegram || open -a Telegram\\ Lite" }]
        },

        {
          "description": "<FN>w := open WeChat",
          "type": "basic",
          "from": { "key_code": "w", "modifiers": { "mandatory": ["fn"], "optional": ["caps_lock"] } },
          "to": [{ "key_code": "vk_none" }, { "shell_command": "open -a WeChat" }]
        }
      ]
    }
  ]
}
